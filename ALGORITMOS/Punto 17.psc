Proceso Inicio_Punto17
	Escribir "Ingrese la Velocidad del Primer Carro en (Km/h)"
	Leer V1
	Escribir "Ingrese la Velocidad del Segundo Carro en (Km/h)"
	Leer V2
	Escribir "Ingrese la distancia en (m)"
	Leer d
	TEncontrarse<-(d/(V1-V2))
	TMinutos<-TEncontrarse*60
	Escribir "El tiempo en encontrarse en horas es" " " TEncontrarse 
	Escribir "El tiempo en encontrarse en minutos es" " " TMinutos
FinProceso
